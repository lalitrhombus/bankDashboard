{
  "name": "create-react-app-redux",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "^1.0.14"
  },
  "dependencies": {
    "chokidar": "^1.7.0",
    "classnames": "^2.2.5",
    "highcharts": "^6.0.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-highcharts": "^13.0.0",
    "react-modal": "^3.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^5.0.0",
    "superagent": "^3.8.0",
    "superagent-graphql": "^0.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "grow-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "watch-css": "npm run build-css && npm run grow-css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": "https://cra-redux-router-thunk.herokuapp.com"
}
